@model Lab05.Models.LtkMember

@{
    ViewBag.Title = "Chi tiết thành viên";
}

<h2>Chi tiết thành viên</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã thành viên
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt>
            Tên đăng nhập
        </dt>
        <dd>
            @Html.DisplayFor(model => model.LtkUsername)
        </dd>

        <dt>
            Họ và tên
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LtkFullName)
        </dd>

        <dt>
            Mật khẩu
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LtkPassword)
        </dd>

        <dt>
            Tuổi
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LtkAge)
        </dd>

        <dt>
            Email
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LtkEmail)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Quay lại", "LtkCreateOne")
</p>
